ault();                if (event.currentTarget.getElementsByClassName('filter-expandable')[0].className === 'triangle-close filter-expandable') {                    element.parentElement.style.height = '';                    element.style.display = 'block';                    event.currentTarget.getElementsByClassName('filter-expandable')[0].className = 'triangle-open filter-expandable';                } else if (event.currentTarget.getElementsByClassName('filter-expandable')[0].className === 'triangle-open filter-expandable') {                    element.parentElement.style.height = '50px';                    element.style.display = 'none';                    event.currentTarget.getElementsByClassName('filter-expandable')[0].className = 'triangle-close filter-expandable';                } else if (event.currentTarget.getElementsByClassName('filter-expandable')[0].className === 'close filter-expandable') {                    element.parentElement.style.height = '';                    element.style.display = 'block';                    event.currentTarget.getElementsByClassName('filter-expandable')[0].className = 'open filter-expandable';                } else {                    element.parentElement.style.height = '20px';                    element.style.display = 'none';                    event.currentTarget.getElementsByClassName('filter-expandable')[0].className = 'close filter-expandable';                }            });        }        /**         * Function to toggle a element in filter container if there are no results         */        function toggle(element, isVisible) {            if (element.className.indexOf('prices-filter') >= 0) {                element.style.display = updatesFromRange || isVisible > 0 ? "block" : "none";            } else {                element.style.display = isVisible ? "block" : "none";            }        }        /**         * Move the results layer to top when a new search is done or filters are changed         */        f
finements widget for desktop         */        search.addWidget(customClearRefinements({            container: document.querySelector('#pcc-search-api--link--close-container')        }));        /**         * Clear refinements widget for mobile         */        search.addWidget(customClearRefinementsMobile({            container: document.querySelector('#pcc-search-api--clear-refinements-mobile')        }));        /**         * Results layer widget with infinite scroll         */        var lastRenderArgs = void 0;        var lastShowMore = true;        var infiniteHits = instantsearch.connectors.connectInfiniteHits(function (renderArgs, isFirstRender) {            var hits = renderArgs.hits,                showMore = renderArgs.showMore,                widgetParams = renderArgs.widgetParams,                container = widgetParams.container;            var emptyLayout = document.getElementById('pcc-search-api--hits-empty');            lastRenderArgs = renderArgs;            if (isFirstRender) {                var wrapper = document.createElement('div');                var olList = document.createElement("ol");                var sectionHits = document.getElementById('section-hits');                wrapper.className = 'ais-Hits';                olList.className = 'ais-Hits-list';                wrapper.appendChild(olList);                container.appendChild(wrapper);                sectionHits.addEventListener('scroll', debounce(function () {                    var threshold = document.getElementsByClassName('ais-Hits-item')[0].clientHeight;                    var sentinel = sectionHits.scrollTop + sectionHits.clientHeight;                    var factor = 2;                    // If scroll beyond the last item and we are not in the last page, show more items                    if (sentinel >= sectionHits.scrollHeight - factor * threshold && !lastRenderArgs.isLastPage) {                        lastShowMore = true;                        showMore();                    }                }, 100), true);                return;            }            if (!lastShowMore) {                scrollToTopHits();            }            lastShowMore = false;            if (isSearchActive) {                container.querySelector('ol').innerHTML = hits.map(function (hit, index) {                    var result = renderArgs.results._rawResults[0];                    hit.queryID = result.queryID;                    hit.hitPosition = index + 1;                    var ellipsis = '';                    if (hit.description.length > 320) {                        ellipsis = '...';                    }                    var description = hit.description.substr(0, 319) + ellipsis;                    return '<li class="ais-Hits-item"><a class="algolia-analytics-link" href="/' + hit.articleUrl + '" data-algolia-objectid="'+ hit.objectID +'" data-algolia-position="'+ hit.hitPosition +'" data-algolia-queryid="'+ hit.queryID +'" data-name="' + hit.title + '" data-id="' + hit.id + '" data-price="' + hit.price.amount + '" data-brand="' + hit.brand.name + '" data-category="' + hit.family.name + '" data-stock_web="' + hit.availability.id + '" data-list="search results" data-eec-action="impression"></a><div class="pcc-search-card">' +                        '<figure><img src="' + hit.imageUrl + '" alt="' + hit.title + '"></figure><div class="content"><div class="title">' + hit.title + '</div><div class="description">' + description + '</div><div class="price">' + parsePrice(hit.price.amount) + ' ‚Ç¨</div></div></li>';                }).join('');            }            var listHits = [].slice.call(document.querySelector('ol').childNodes);            listHits.forEach(function(item) {                item.addEventListener('click', function(){                    localStorage.setItem('dataAlgoliaQueryId',item.childNodes[0].getAttribute('data-algolia-queryid'))                    localStorage.setItem('dataAlgoliaObjectId',item.childNodes[0].getAttribute('data-algolia-objectid'))                    window.analytics("clickedObjectIDsAfterSearch", {                        index: search.indexName,                        eventName: "click-result",                        objectIDs: [item.childNodes[0].getAttribute('data-algolia-objectid')],                        queryID: item.childNodes[0].getAttribute('data-algolia-queryid'),                        positions: [parseInt(item.childNodes[0].getAttribute('data-algolia-position'))]                    });                })            })            if (hits.length === 0) {                emptyLayout.innerText = 'Lo sentimos, no encontramos resultados';            } else {                emptyLayout.innerText = '';            }        });        var infiniteHitsMobile = instantsearch.connectors.connectInfiniteHits(function (renderArgs, isFirstRender) {            var hits = renderArgs.hits,                showMore = renderArgs.showMore,                widgetParams = renderArgs.widgetParams,                container = widgetParams.container;            var emptyLayout = document.querySelectorAll('#pcc-search-api--hits-empty')[1];            lastRenderArgs = renderArgs;            if (isFirstRender) {                var wrapper = document.createElement('div');                var olList = document.createElement("ol");                var sectionMobileHits = document.getElementById('pcc-search-api--container-mobile');                wrapper.className = 'ais-Hits';                olList.className = 'ais-Hits-list';                olList.setAttribute('data-impressions-block-container', 'search results');                wrapper.appendChild(olList);                container.appendChild(wrapper);                sectionMobileHits.addEventListener('scroll', throttle(function () {                    var threshold = document.querySelector('.ais-Hits-item').clientHeight + 50;                    var sentinel = sectionMobileHits.scrollTop + sectionMobileHits.clientHeight;                    if (sectionMobileHits.scrollTop > 100) {                        toggleButtonsBottom(true);                    } else {                        toggleButtonsBottom(false);                    }                    // If scroll beyond the last item and we are not in the last page, show more items                    if (sentinel >= wrapper.clientHeight - threshold && !lastRenderArgs.isLastPage) {                        lastShowMore = true;                        showMore();                    }                }, 100));                return;            }            if (!lastShowMore) {                scrollToTopHits();            }            lastShowMore = false;            container.querySelector('ol').innerHTML = hits.map(function (hit, index) {                var result = renderArgs.results._rawResults[0];                var offset = result.hitsPerPage * result.page;                hit.queryID = result.queryID;                hit.hitPosition = offset + index + 1;                return '<li class="ais-Hits-item"><a class="algolia-analytics-link" href="/' + hit.articleUrl + '" data-algolia-objectid="'+ hit.objectID +'" data-algolia-position="'+ hit.hitPosition +'" data-algolia-queryid="'+ hit.queryID +'" data-name="' + hit.title + '" data-id="' + hit.id + '" data-price="' + hit.price.amount + '" data-brand="' + hit.brand.name + '" data-category="' + hit.family.name + '" data-stock_web="' + hit.availability.id + '" data-list="search results" data-eec-action="impression"></a><div class="pcc-search-card">' +                    '<figure><img src="' + hit.imageUrl + '" alt="' + hit.title + '"></figure><div class="content"><div class="title">' + hit.title + '</div><div class="price">' + parsePrice(hit.price.amount) + ' ‚Ç¨</div></div></li>';            }).join('');            var listHits = [].slice.call(document.querySelector('ol').childNodes);            listHits.forEach(function(item) {                item.addEventListener('click', function(){                    localStorage.setItem('dataAlgoliaQueryId',item.childNodes[0].getAttribute('data-algolia-queryid'))                    localStorage.setItem('dataAlgoliaObjectId',item.childNodes[0].getAttribute('data-algolia-objectid'))                    window.analytics("clickedObjectIDsAfterSearch", {                        index: search.indexName,                        eventName: "click-result",                        objectIDs: [item.childNodes[0].getAttribute('data-algolia-objectid')],                        queryID: item.childNodes[0].getAttribute('data-algolia-queryid'),                        positions: [parseInt(item.childNodes[0].getAttribute('data-algolia-position'))]                    });                })            })            if (hits.length === 0) {                emptyLayout.innerText = 'Lo sentimos, no encontramos resultados';            } else {                emptyLayout.innerText = '';            }        });        if (window.screen.width > windowMobileWidth) {            search.addWidget(                infiniteHits({                    container: document.querySelector('#pcc-search-api--hits')                })            );        } else {            search.addWidget(                infiniteHitsMobile({                    container: document.querySelector('#pcc-search-api--hits-mobile')                })            );        }        /**         * Clear the searches         */        function resetSearches(helper) {            helper                .setQuery("") // update the parameters                .search(); // launch the query        }        function addQueryBySpeech(helper, query) {            helper                .setQuery(query) // update the parameters                .search(); // launch the query        }        /**         * Initiation of the search         */        search.start();        var formDesktop = document.getElementsByClassName('ais-SearchBox-form')[0];        var searchInput = document.getElementsByClassName('pcc-search-api--input-text')[0];        var searchInputMobile = document.getElementsByClassName('ais-SearchBox pcc-search-api--input-mobile')[0];        formDesktop.removeAttribute(('novalidate'));        searchInput.setAttribute('required', true);        searchInput.setCustomValidity("Completa este campo");        formDesktop.setAttribute('action', window.location.origin + '/buscar/');        searchInputMobile.setAttribute('name', 'query');        searchInputMobile.setAttribute('data-url', window.location.origin + '/buscar/');        searchInputMobile.addEventListener('keypress', function (e) {            if (e.which == 13 && searchInput.value != '') {                formDesktop.submit();                e.preventDefault();            }        });        searchInput.setAttribute('name', 'query');        searchInput.setAttribute('data-url', window.location.origin + '/buscar/');        formDesktop.addEventListener('keypress', function (e) {            if (e.which == 13 && searchInput.value != '') {                formDesktop.submit();                e.preventDefault();            }        });        /**         * Listener to capture typing in the search box for mobile, to close or show the close/clear button         */        var searchBoxMobile = document.getElementById('pcc-search-api--input-mobile').querySelector("input");        searchBoxMobile.addEventListener('click', function (event) {            onChangeInputMobile(event);        });        searchBoxMobile.addEventListener('keyup', function (event) {            onChangeInputMobile(event);        });        function onChangeInputMobile(event) {            var buttons = getButtonClearClose(),                buttonClear = buttons.buttonClear,                buttonClose = buttons.buttonClose;            if (event.target.value.length > 0) {                buttonClear.style.display = 'block';                buttonClose.style.display = 'none';            } else {                event.target.value = '';                buttonClear.style.display = 'none';                buttonClose.style.display = 'block';            }        }        function clearInputMobile() {            var buttons = getButtonClearClose(),                buttonClear = buttons.buttonClear,                buttonClose = buttons.buttonClose;            searchBoxMobile.value = '';            buttonClear.style.display = 'none';            buttonClose.style.display = 'block';        }        function getButtonClearClose() {            var buttons = document.getElementById('pcc-search-api--wrapper-input').querySelectorAll("button"),                buttonClose = null,                buttonClear = null;            buttons.forEach(function (button) {                if (button.dataset.role === 'close') {                    buttonClose = button;                } else if (button.dataset.role === 'clear') {                    buttonClear = button;                }            });            return {buttonClose: buttonClose, buttonClear: buttonClear};        }        function toggleButtonsBottom(shown) {            var buttons = document.querySelectorAll('.pcc-search-api--bottom-button');            buttons.forEach(function (button) {                if (shown) {                    button.classList.add('in');                } else {                    button.classList.remove('in');                }            });        }        function scrollToTop(scrollDuration, callback) {            toggleButtonsBottom(false);            var element = document.getElementById('pcc-search-api--container-mobile');            var scrollStep = -element.scrollTop / (scrollDuration / 15),                scrollInterval = setInterval(function () {                    if (element.scrollTop != 0) {                        element.scrollBy(0, scrollStep);                    } else {                        clearInterval(scrollInterval);                        callback();                    }                }, 15);        }        /**         * Custom widget for clear searches         */        search.addWidget({            init: function (opts) {                var helper = opts.helper;                var closeMobile = document.querySelector("#pcc-search-api--close-mobile");                var closeMobileInput = document.querySelector("#pcc-search-api--reset-mobile-input");                var inputSearchMobile = document.querySelector("#" + idInputMobile);                var inputSearch = document.querySelector("#" + idInput);                closeMobile.addEventListener("click", function (e) {                    closeSearch();                    clearInputMobile();                    resetSearches(helper);                });                inputSearch.addEventListener("input", function (e) {                    var clearButton = document.getElementById('pcc-search-api--clear-refinements-mobile').querySelector("button");                    triggerEvent(clearButton, 'click');                });                inputSearchMobile.addEventListener("input", function (e) {                    var clearButton = document.getElementById('pcc-search-api--clear-refinements-mobile').querySelector("button");                    triggerEvent(clearButton, 'click');                });                closeMobileInput.addEventListener("click", function (e) {                    resetSearches(helper);                });                searchBoxMobile.addEventListener("click", function (e) {                    addQueryBySpeech(helper, e.target.value)                });                /**                 * Button for open the search layer in Mobile                 */                openSearchMobileButton.addEventListener('click', function (event) {                    event.preventDefault();                    containerMobile.style.display = 'block';                    openSearchMobile();                    resetSearches(helper);                });            }        });        /**         * @param price         * @returns {string}         */        function parsePrice(price) {            var fixed = ((price * 100) % 100 == 0) ? 0 : 2;            return price.toFixed(fixed).toString().replace(".", ",");        }        /**         * Change the price format on footer of algolia range slider widget         */        function changePriceFormatOnRangeSliderWidget() {            var priceElements = document.getElementsByClassName('rheostat-value');            var nodespriceElements = [].slice.call(priceElements);            nodespriceElements.forEach(function (priceElement) {                priceElement.innerText = parsePrice(parseFloat(priceElement.innerText));            })        }        /**         * Set the local storage when click over product and send the analytic         * @param queryId, id with the content of Algolia query ID         * @param objectId, product ID         * @param position, product position in the list         */        function clickedObjectIDsAfterSearch(objectId, position) {            var queryID = localStorage.getItem('queryID-last-search');            localStorage.setItem('dataAlgoliaQueryId',queryID);            localStorage.setItem('dataAlgoliaObjectId',objectId)            window.analytics("clickedObjectIDsAfterSearch", {                    index: search.indexName,                    eventName: "click-result",                    objectIDs: [objectId],                    queryID: queryID,                    positions: [parseInt(position)]                });        }    }</script><script type="application/ld+json" id="microdata-breadcrumblist-script">    {        "@context": "http://schema.org",        "@type": "BreadcrumbList",        "itemListElement": [                    {            "@type": "ListItem",            "position": 0,            "item": {                            "@id": "https://www.pccomponentes.com/",                            "name": "Home"            }        }            ,        {            "@type": "ListItem",            "position": 1,            "item": {                            "@id
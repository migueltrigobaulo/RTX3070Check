      };                var callNow = immediate && !timeout;                clearTimeout(timeout);                timeout = setTimeout(later, wait);                if (callNow) {                    func.apply(context, args);                }            };        };        /**         * Events to handle buttons in search layer mobile         */        // Handler for web speech button        document.getElementById('pcc-search-api-web-speech').addEventListener('click', function () {            toggleWebSpeech();        });        var buttonsMobileSearch = document.getElementsByClassName('pcc-search-api--button-mobile');        var nodesMobileButton = [].slice.call(buttonsMobileSearch);        nodesMobileButton.forEach(function (node) {            node.addEventListener('click', function (event) {                event.preventDefault();                switch (node.dataset.role) {                    case 'clear':                        clearInputMobile();                        break;                    case 'close':                        containerMobile.style.display = 'none';                        break;                    case 'close-filter':                        toggleFiltersMobile(false);                        break;                }            })        });        var buttonsBottomMobile = document.querySelectorAll('.pcc-search-api--bottom-button');        buttonsBottomMobile.forEach(function (button) {            button.addEventListener('click', function (event) {                event.preventDefault();                switch (button.dataset.role) {                    case 'scrolltop':                        scrollToTop(250, function () {                            toggleButtonsBottom(false);                        });                        break;                    case 'close':                        scrollToTop(250, function () {                            containerMobile.style.display = 'none';                        });                        break;       